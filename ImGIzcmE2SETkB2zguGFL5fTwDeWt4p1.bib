@techreport{ritchieSexDifferencesAdult2017,
  title = {Sex Differences in the Adult Human Brain: {{Evidence}} from 5,216 {{UK Biobank}} Participants},
  shorttitle = {Sex Differences in the Adult Human Brain},
  author = {Ritchie, Stuart J. and Cox, Simon R. and Shen, Xueyi and Lombardo, Michael V. and Reus, Lianne M. and Alloza, Clara and Harris, Mathew A. and Alderson, Helen L. and Hunter, Stuart and Neilson, Emma and Liewald, David C. M. and Auyeung, Bonnie and Whalley, Heather C. and Lawrie, Stephen M. and Gale, Catharine R. and Bastin, Mark E. and McIntosh, Andrew M. and Deary, Ian J.},
  year = {2017},
  month = apr,
  institution = {{Neuroscience}},
  doi = {10.1101/123729},
  abstract = {Sex differences in human brain structure and function are of substantial scientific interest because of sex-differential susceptibility to psychiatric disorders [1,2,3] and because of the potential to explain sex differences in psychological traits [4]. Males are known to have larger brain volumes, though the patterns of differences across brain subregions have typically only been examined in small, inconsistent studies [5]. In addition, despite common findings of greater male variability in traits like intelligence [6], personality [7], and physical performance [8], variance differences in the brain have received little attention. Here we report the largest single-sample study of structural and functional sex differences in the human brain to date (2,750 female and 2,466 male participants aged 44-77 years). Males had higher cortical and sub-cortical volumes, cortical surface areas, and white matter diffusion directionality; females had thicker cortices and higher white matter tract complexity. Considerable overlap between the distributions for males and females was common, and subregional differences were smaller after accounting for global differences. There was generally greater male variance across structural measures. The modestly higher male score on two cognitive tests was partly mediated via structural differences. Functional connectome organization showed stronger connectivity for males in unimodal sensorimotor cortices, and stronger connectivity for females in the default mode network. This large-scale characterisation of neurobiological sex differences provides a foundation for attempts to understand the causes of sex differences in brain structure and function, and their associated psychological and psychiatric consequences.},
  file = {C\:\\Users\\the_l\\Zotero\\storage\\IUWSY6JH\\Ritchie et al. - 2017 - Sex differences in the adult human brain Evidence.pdf;C\:\\Users\\the_l\\Zotero\\storage\\Z6R5PPWJ\\Ritchie et al. - 2017 - Sex differences in the adult human brain Evidence.pdf},
  language = {en},
  type = {Preprint}
}

